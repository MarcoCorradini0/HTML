{
  "version": 2,
  "routes": [
    {
      "src": "/api/proxy",
      "methods": ["GET", "POST"],
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "/api/proxy",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy.js"
    }
  ]
}
